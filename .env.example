# Redis connection string for Vercel deployment
# Get this from your Redis provider (e.g., Upstash)
# Format: redis://default:password@host:port
REDIS_URL=

# API Key for authentication (REQUIRED)
# Generate a strong random key: openssl rand -base64 32
API_KEY=

# Frontend API Key (must match backend API_KEY for authentication to work)
VITE_API_KEY=
